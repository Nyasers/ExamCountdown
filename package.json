{
  "name": "examcountdown",
  "version": "24.11.25",
  "description": "Electron shell for ExamCountdown",
  "main": "main.js",
  "type": "module",
  "config": {
    "forge": "forge.config.js"
  },
  "scripts": {
    "build": "npm install && cd website && npm run build && copy /y dist\\index.html .. && cd ..",
    "start": "npm run build && electron-forge start",
