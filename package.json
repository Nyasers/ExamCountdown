{
  "name": "examcountdown",
  "private": true,
  "version": "25.3.28",
  "scripts": {
    "tauri": "tauri",
    "pretauri": "npm run build",
    "prebuild": "npm install && (mkdir dist || del /s /q dist) || echo.",
    "build": "npm run build:main && npm run build:editor",
    "postbuild": "node src-tauri\\version.js",
    "build:main": "npm run build --workspace=src-main && copy src-main\\dist\\index.html dist && copy src-main\\dist\\*.js dist",
    "build:editor": "npm run build --workspace=src-editor && copy src-editor\\dist\\index.min.html dist\\editor.html",
    "clean": "npm run clean:main && npm run clean:editor && npm run clean:tauri",
    "clean:main": "npm run clean --workspace=src-main",
    "clean:editor": "npm run clean --workspace=src-editor",
    "clean:tauri": "cd src-tauri && cargo clean",
    "postclean": "rmdir /s /q dist node_modules || echo.",
    "start": "npm run tauri build",
    "test": "npm run tauri dev"
  },
  "author": "Nyasers",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@actions/github": "^6.0.0",
    "@iarna/toml": "^2.2.5",
    "@tauri-apps/cli": "^2.4.1"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.4.1",
    "@tauri-apps/plugin-autostart": "^2.3.0",
    "@tauri-apps/plugin-process": "^2.2.1",
    "@tauri-apps/plugin-store": "^2.2.0",
    "@tauri-apps/plugin-updater": "^2.7.0",
    "tauri-plugin-wallpaper": "^2.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nyasers/ExamCountdown.git"
  },
  "keywords": [],
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Nyasers/ExamCountdown/issues"
  },
  "homepage": "https://github.com/Nyasers/ExamCountdown#readme",
  "workspaces": [
    "src-editor",
    "src-main"
  ]
}
