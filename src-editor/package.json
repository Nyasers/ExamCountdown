{
  "name": "config-editor",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev",
    "prebuild": "npm install",
    "build": "rsbuild build",
    "postbuild": "node build.js && npm run minify-html",
    "preview": "rsbuild preview",
    "minify-html": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --minify-css true --minify-js true .\\dist\\index.html -o .\\dist\\index.min.html && node minify.js",
    "clean": "rmdir /s /q dist node_modules || echo."
  },
  "dependencies": {
    "@ant-design-vue/use": "^0.0.1-alpha.10",
    "@lljj/vue3-form-ant": "^1.19.0",
    "ant-design-vue": "^4.2.6",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@rsbuild/core": "^1.3.0",
    "@rsbuild/plugin-babel": "^1.0.4",
    "@rsbuild/plugin-vue": "^1.0.7",
    "@rspack/cli": "^1.3.0",
    "@rspack/core": "^1.3.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-import": "^1.13.8",
    "html-minifier": "^4.0.0",
    "terser": "^5.39.0",
    "unplugin-auto-import": "^19.1.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
