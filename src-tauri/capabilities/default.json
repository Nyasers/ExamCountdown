{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:app:allow-default-window-icon",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-show",
    "wallpaper:default"
  ]
}