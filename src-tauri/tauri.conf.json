{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ExamCountdown",
  "version": "25.1.12",
  "identifier": "top.nyaser.ec",
  "build": {
    "frontendDist": "../website/dist",
    "beforeDevCommand": "npm run build",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ExamCountdown",
        "width": 1280,
        "height": 720,
        "visible": false,
        "resizable": true,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}