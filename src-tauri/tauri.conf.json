{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ExamCountdown",
  "version": "25.2.4",
  "identifier": "top.nyaser.ec",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run build",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "url": "index.html",
        "title": "ExamCountdown",
        "width": 1280,
        "height": 720,
        "visible": false,
        "resizable": true,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": {
        "languages": [
          "SimpChinese",
          "TradChinese",
          "English"
        ]
      }
    }
  }
}